class Minimum{
     public static void main (String args[]) {
        int a;
        Scanner s=new Scanner(System.in);
        System.out.println("ENTER THE NUMBER OF ELEMENTS TO BE CHECKED");
        a=s.nextInt();
        int ar[];
        ar=new int[a];
        System.out.println("ENTER THE ELEMENTS:");
        for(int i=0;i<a;i++){
            ar[i]=s.nextInt();
        }
        Arrays.sort(ar);
        System.out.format("sorted %d",+ar[0]);
        
        
    }
}
